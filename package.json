{
  "name": "jordangarcia.me",
  "private": true,
  "description": "Jordan's personal website",
  "version": "1.0.0",
  "author": "Jordan Garcia <arickho@gmail.com>",
  "repository": "https://github.com/arickho/whoisjordangarcia.me",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{ts,tsx,md,mdx,json,css}\"",
    "validate": "npm run lint && npm run stylelint",
    "lint": "eslint --ignore-path .gitignore --fix \"./src/**/*.{ts,tsx}\"",
    "stylelint": "stylelint --syntax css --fix \"./src/**/*.css\"",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "gatsby": "^2.21.33",
    "gatsby-image": "^2.4.3",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-google-analytics": "^2.3.1",
    "gatsby-plugin-google-tagmanager": "^2.3.1",
    "gatsby-plugin-manifest": "^2.4.3",
    "gatsby-plugin-offline": "^3.2.2",
    "gatsby-plugin-react-helmet": "^3.3.1",
    "gatsby-plugin-sharp": "^2.6.3",
    "gatsby-plugin-typescript": "^2.4.2",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^3.3.3",
    "gatsby-source-filesystem": "^2.3.3",
    "gatsby-transformer-remark": "^2.8.8",
    "gatsby-transformer-sharp": "^2.5.2",
    "gsap": "^3.6.0",
    "imagemin-pngquant": "^8.0.0",
    "pngquant-bin": "^5.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0"
  },
  "devDependencies": {
    "@types/gsap": "^1.20.2",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "stylelint": "^13.3.3",
    "stylelint-config-standard": "^20.0.0",
    "typescript": "^3.9.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
