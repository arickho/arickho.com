{
  "presets": ["next/babel"],
  "plugins": [
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false
      }
    ],
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "components": "./components",
          "images": "./images",
          "lib": "./lib",
          "sections": "./sections",
          "static": "./static",
          "store": "./store",
          "styles": "./components/_styles",
          "templates": "./components/_templates"
        }
      }
    ]
  ]
}
